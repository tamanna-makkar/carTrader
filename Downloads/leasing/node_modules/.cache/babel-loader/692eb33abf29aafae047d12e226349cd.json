F,CAA6C,CAArE,EAA0B;AAAA;AAA1B,G,qCAwBI,E,CAvBM,IAAA,CAAA,Q,kBAAV,YAAA,CAA8E,GAA9E,EAAA,UAAA,EAA8E,gBAAA,CAAnC,IAAA,CAAA,EAAA,CAAE,wBAAF,CAAmC,CAA9E,EAA6C;AAAA;AAA7C,G,qCAuBI,EAtBJ,YAAA,CAqBM,KArBN,EAAA,UAAA,EAqBM,CApBJ,YAAA,CAea,qBAfb,EAAA,WAAA,CAea;AAdX,IAAA,GAAG,EAAC,QAcO;gBAbF,IAAA,CAAA,WAaE;;aAbF,IAAA,CAAA,WAAA,GAAW,M;MAaT;AAZV,IAAA,KAAK,EAAE,IAAA,CAAA;AAYG,GAfb,EAIU,IAAA,CAAA,iBAJV,EAI2B;AACzB,IAAA,OAAO,EAAC,QADiB;AAEzB,IAAA,SAAS,EAAC;AAFe,GAJ3B,CAAA,E;AAQmB,IAAA,OAAO,EAAA,OAAA,CACtB;AAAA,UAD0B,KAC1B,QAD0B,KAC1B;AAAA,aAD+B,CAC/B,YAAA,CAIM,KAJN,EAAA,UAAA,EAIM,CAHQ,KAAK,IAAA,C,kBAAjB,YAAA,CAA6F,MAA7F,EAAA,UAAA,EAA6F,gBAAA,CAAlB,KAAkB,CAAA,GAAV,KAAnF,EAAsF;AAAA;AAAtF,O,IACiB,KAAK,IAAA,I,kBAAtB,YAAA,CAAoG,MAApG,EAAA,UAAA,EAAoG,gBAAA,CAAlB,KAAkB,CAAA,GAAV,KAA1F,EAA6F;AAAA;AAA7F,O,mBACA,YAAA,CAAsE,MAAtE,EAAA,UAAA,EAAsE,gBAAA,CAAlB,KAAkB,CAAA,GAAV,KAA5D,EAA+D;AAAA;AAA/D,O,CACI,CAJN,CAD+B,CAC/B;AAAA,KADsB,C;;;;GAR1B,E;;AAAA,I,uBAAA,CAoBI,EAJJ,UAII,CArBN,CAsBI,CAzBN,C","sourcesContent":["<template>\n  <div class=\"wizard-inner-wrap step7\">\n    <h3 v-if=\"!isFilter\">{{ $t('_wizard.step7.minTowingCapacity') }}</h3>\n    <p v-if=\"!isFilter\" class=\"sub-heading\">{{ $t('_wizard.step7.subTitle') }}</p>\n    <div class=\"slider-wrapper\">\n      <vue-slider\n        ref=\"slider\"\n        v-model=\"sliderValue\"\n        :vData=\"sliderData\"\n        v-bind=\"sliderDataOptions\"\n        tooltip=\"always\"\n        direction=\"rtl\"\n      >\n        <template v-slot:tooltip=\"{ value }\">\n          <div class=\"vue-slider-dot-tooltip-inner\">\n            <span v-if=\"value == 0\" class=\"first-index vue-slider-dot-tooltip-text\">{{ value }} kg</span>\n            <span v-else-if=\"value == 3500\" class=\"last-index vue-slider-dot-tooltip-text\">{{ value }} kg</span>\n            <span v-else class=\"vue-slider-dot-tooltip-text\">{{ value }} kg</span>\n          </div>\n        </template>\n      </vue-slider>\n      <div class=\"placeholders\">\n        <span class=\"t2\">0 kg</span>\n        <span class=\"t2\">3500 kg</span>\n      </div>\n    </div>\n  </div>\n</template>\n<script lang=\"ts\">\nimport { defineComponent, ref, reactive, watch } from 'vue'\nimport VueSlider from 'vue-slider-component'\nimport sliderOptions from '@/mixins/sliderOptions'\nexport default defineComponent({\n  components: {\n    VueSlider\n  },\n  props: {\n    isFilter: {\n      type: Boolean,\n      default: false\n    }\n  },\n  setup() {\n    const sliderValue = sessionStorage.getItem('towing_capacity')\n      ? ref(JSON.parse(sessionStorage.getItem('towing_capacity') || ''))\n      : ref([0])\n    const sliderData = reactive([\n      3500, 3400, 3300, 3200, 3100, 3000, 2900, 2800, 2700, 2600, 2500, 2400, 2300, 2200, 2100, 2000, 1900, 1800, 1700,\n      1600, 1500, 1400, 1300, 1200, 1100, 1000, 900, 800, 700, 600, 500, 0\n    ])\n    watch(\n      () => sliderValue.value,\n      (currentValue) => {\n        sessionStorage.setItem('towing_capacity', JSON.stringify(currentValue))\n        if (currentValue == 0) {\n          sessionStorage.removeItem('towing_capacity')\n        }\n      }\n    )\n    const { sliderDataOptions } = sliderOptions()\n    return { sliderValue, sliderDataOptions, sliderData }\n  }\n})\n</script>\n<style lang=\"sass\" scoped>\n.step7\n  h3\n    padding-bottom: 0.55em\n  p\n    text-align: center\n    font-size: 1.4em\n    margin-bottom: 2em\n</style>"],"sourceRoot":""}]}